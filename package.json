{
  "name": "vite-react-wasm",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:wasm": "wasm-pack build wasm",
    "dev": "vite",
    "eslint": "eslint \"{src,mock,tests}/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"{src,mock,tests}/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write \"**/*.{js,jsx,css,less,sass,scss,json}\"",
    "prettier:ts": "prettier --parser=typescript --write \"**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "stylelint": "stylelint \"src/**/*.{css,less,sass,scss}\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.{css,less,sass,scss}\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wasm": "link:wasm/pkg"
  },
  "devDependencies": {
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.12",
    "sass": "^1.72.0",
    "stylelint": "^16.2.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "^0.12.1"
  }
}
