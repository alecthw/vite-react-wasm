{
  "name": "vite-react-wasm",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:wasm": "wasm-pack build wasm",
    "dev": "vite",
    "eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write \"**/*.{js,jsx,css,less,sass,scss,json}\"",
    "prettier:ts": "prettier --parser=typescript --write \"**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "stylelint": "stylelint \"src/**/*.{css,less,sass,scss}\"",
    "stylelint:fix": "stylelint --fix \"src/**/*.{css,less,sass,scss}\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "wasm": "link:wasm/pkg"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.3",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "^0.13.0"
  }
